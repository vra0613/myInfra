def installedPlugins = Jenkins.instance.pluginManager.plugins
installedPlugins.each { plugin ->
  def dependencies = plugin.getDependencies()
  def dependenciesInfo = dependencies.collect {it.shortName + ':' + it.version}
 
  println("Plugin- ${plugin.getShortName()}:${plugin.getVersion()}")
  println("URL- ${plugin.getUrl()}")
  println("Dependencies- ${dependenciesInfo.join(', ')}")
  println("----")
}
